<div class="panel panel-default">
	{{#title}}
	<div class="panel-heading">
		<h3 class="panel-title">{{title}}</h3>
	</div>
	{{/title}}
	
	{{#body}}
	<div class="panel-body">
		{{{body}}}
	</div>
	{{/body}}
	
	{{#hasvaluelist}}
	<table class="table">
    	<thead>
    		<tr>
    			<th>Name</th><th>Description</th><th>Value</th>
    		</tr>
    	</thead>
    	<tbody>
    		{{#valuelist}}
    			<tr>
    				<td>{{name}}</td><td>{{{description}}}</td><td>{{value}}</td>
    			</tr>
    		{{/valuelist}}
		</tbody>
	</table>
	{{/hasvaluelist}}
</div>